{
  "name": "@farmfe/plugin-component",
  "version": "0.0.1",
  "main": "scripts/index.js",
  "types": "scripts/index.d.ts",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/farm-fe/plugins",
  "author": "asasugar <xxj95719@gmail.com>",
  "bugs": {
    "url": "https://github.com/farm-fe/plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farm-fe/plugins.git"
  },
  "keywords": [
    "rust",
    "component",
    "farmfe"
  ],
  "devDependencies": {
    "@farmfe/plugin-tools": "latest"
  },
  "napi": {
    "name": "farmfe_plugin_component",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./scripts/func.js",
      "types": "./scripts/index.d.ts",
      "default": "./scripts/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "cargo watch -w src -s 'scripts/watch.sh'",
    "bump": "npx changeset version",
    "play": "pnpm --filter playground dev",
    "build": "farm-plugin-tools build --platform --cargo-name farmfe_plugin_component -p farmfe_plugin_component --release",
    "prepublishOnly": "farm-plugin-tools prepublish"
  },
  "files": [
    "scripts",
    "options.d.ts"
  ]
}
